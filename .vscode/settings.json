  "eslint.format.enable": true,
  "editor.formatOnSave": true,
  "editor.rulers": [
    120
  ],
  "editor.wordWrapColumn": 120,
  "yaml.schemas": {},
  "typescript.tsdk": "node_modules/typescript/lib"
}